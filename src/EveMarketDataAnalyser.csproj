<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <EnableWindowsTargeting>true</EnableWindowsTargeting>
    <Nullable>enable</Nullable>
    <AssemblyName>EveMarketDataAnalyser</AssemblyName>
    <PackageId>EveMarketDataAnalyser</PackageId>
    <ToolCommandName>EveMarketDataAnalyser</ToolCommandName>
    <Description>Analyse le market de stations dans eve online, pour donner la disponibiliter d item.</Description>
    <UseWindowsForms>True</UseWindowsForms>
    <StartupObject>MarketDataAnalyser.Program</StartupObject>
    
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ESI.NET" Version="2023.2.11" />
    <PackageReference Include="ObjectListView.Official" Version="2.9.1" />
  
  </ItemGroup>

</Project>